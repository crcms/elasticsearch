{
  "name": "crcms/elasticsearch",
  "description": "Use SQL statements to query elasticsearch",
  "keywords": [
    "elasticsearch",
    "laravel",
    "sql",
    "orm"
  ],
  "homepage": "https://github.com/crcms/elasticsearch",
  "license": "MIT",
  "authors": [
    {
      "name": "simon",
      "email": "simon@crcms.cn"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/support": "^5.3|^6.0|^7.0|^8.0",
    "elasticsearch/elasticsearch": "~7.0",
    "ramsey/uuid": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "symfony/var-dumper": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "CrCms\\ElasticSearch\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CrCms\\ElasticSearch\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "CrCms\\ElasticSearch\\LaravelServiceProvider"
      ],
      "aliases": {
      }
    }
  }
}
